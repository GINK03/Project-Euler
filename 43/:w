import sys, math, os, itertools

base = '123456789'

def isDividable(n):
    for i in xrange(2, int(math.sqrt(n)) + 1):
        if n%i == 0:
            return i
    return None

for p in itertools.permutations(base):
    eval_num = ''.join(p)
    e1, e2, e3 = map(lambda x:int(x), [eval_num[0:3],eval_num[3:6],eval_num[6:9]])
    print e1, e2, e3
    if isDividable(e1) and isDividable(e2) and isDividable(e3):
        print 'this is not devidable', eval_num
